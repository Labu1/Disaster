	focus = {
		id = AUT_merge_the_posts
		#icon = 
		x = 8
		y = 0
		cost = 7
		ai_will_do = {
			factor = 100
		}
		select_effect = {
			country_event = { id = aut.2 days = 5 }
		}
		search_filters = { }
		completion_reward = {
			custom_effect_tooltip = AUT_posts_merged
			else_if = {
				limit = { has_country_flag = AUT_dollfuss_radio_address }
				country_event = aut.8
			}	
		    else_if = {
				limit = { has_country_flag = AUT_dollfuss_private_ceremony }
				country_event = aut.9
			}	
        }
	}
	
	focus = {
