			}					
		}	
		search_filters = { FOCUS_FILTER_INDUSTRY }
		completion_reward = {
			custom_effect_tooltip = AUT_economic_recovery
			if = {
				limit = { has_idea = AUT_sluggish_economy_1 }
				swap_ideas = { 
                    remove_idea = AUT_sluggish_economy_1 
                    add_idea = AUT_sluggish_economy_2
                }
			}
			else_if = {
				limit = { has_idea = AUT_sluggish_economy_2 }
				swap_ideas = { 
                    remove_idea = AUT_sluggish_economy_2 
